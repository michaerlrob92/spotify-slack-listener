{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,5433;Database=SpotifySlackListener;User Id=sa;Password=Pass@word"
  },
  "StatusBackgroundService": {
    "UpdatePoll": 2000
  },
  "Spotify": {
    "AuthorizeUri": "https://accounts.spotify.com/authorize",
    "TokenUri": "https://accounts.spotify.com/api/token",
    "ProfileUri": "https://api.spotify.com/v1/me",
    "PlayerUri": "https://api.spotify.com/v1/me/player",
    "RedirectUri": "https://localhost:5001/spotify-callback",
    "Scopes": "user-read-currently-playing user-read-playback-state"
  },
  "Slack": {
    "AuthorizeUri": "https://slack.com/oauth/authorize",
    "TokenUri": "https://slack.com/api/oauth.access",
    "ProfileUri": "https://slack.com/api/users.profile.set",
    "RedirectUri": "https://localhost:5001/slack-callback",
    "Scopes": "users.profile:write"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
